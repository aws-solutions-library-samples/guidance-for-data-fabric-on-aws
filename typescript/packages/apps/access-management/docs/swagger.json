{
  "openapi": "3.0.3",
  "info": {
    "title": "DF Credentials Provider",
    "description": "API to manage credentials vended by DF",
    "version": "0.0.1"
  },
  "components": {
    "schemas": {
      "def-0": {
        "type": "object",
        "properties": {
          "AccessKeyId": {
            "description": "The access key ID that identifies the temporary security credentials.",
            "type": "string"
          },
          "Expiration": {
            "description": "The date on which the current credentials expire.",
            "type": "string"
          },
          "SecretAccessKey": {
            "description": "The secret access key that can be used to sign requests.",
            "type": "string"
          },
          "SessionToken": {
            "description": "The token that users must pass to the service API to use the temporary credentials.",
            "type": "string"
          }
        },
        "required": [
          "AccessKeyId",
          "Expiration",
          "SecretAccessKey",
          "SessionToken"
        ],
        "title": "credentials"
      }
    }
  },
  "paths": {
    "/credentials": {
      "post": {
        "operationId": "generateCredentials",
        "tags": [
          "AccessManagement"
        ],
        "description": "Generate credentials",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "projectId",
            "required": true,
            "description": "Filters processed activities that contributed to a specific metric."
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "assetListingId",
            "required": true,
            "description": "Filters processed activities based on a specific pipeline execution."
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "accept-version",
            "required": true,
            "description": "API version"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "accept",
            "required": true,
            "description": "Accepted Content Type"
          }
        ],
        "responses": {
          "201": {
            "description": "Success.",
            "content": {
              "application/json": {
                "schema": {
                  "description": "Success.",
                  "id": "1234",
                  "type": "object",
                  "properties": {
                    "AccessKeyId": {
                      "description": "The access key ID that identifies the temporary security credentials.",
                      "type": "string"
                    },
                    "Expiration": {
                      "description": "The date on which the current credentials expire.",
                      "type": "string"
                    },
                    "SecretAccessKey": {
                      "description": "The secret access key that can be used to sign requests.",
                      "type": "string"
                    },
                    "SessionToken": {
                      "description": "The token that users must pass to the service API to use the temporary credentials.",
                      "type": "string"
                    }
                  },
                  "required": [
                    "AccessKeyId",
                    "Expiration",
                    "SecretAccessKey",
                    "SessionToken"
                  ]
                },
                "examples": {
                  "New credentials": {
                    "summary": "New credentials",
                    "value": {
                      "AccessKeyId": "lweoipqophgFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY",
                      "SecretAccessKey": "wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY",
                      "SessionToken": "AQoEXAMPLEH4aoAH0gNCAPyJxz4BlCFFxWNE1OPTgk5TthT",
                      "Expiration": "2023-01-01T00:00:00.000Z"
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "Forbidden.",
            "content": {
              "application/json": {
                "schema": {
                  "description": "Forbidden.",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ]
                }
              }
            }
          },
          "404": {
            "description": "Not found.",
            "content": {
              "application/json": {
                "schema": {
                  "description": "Not found.",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string"
                    }
                  },
                  "required": [
                    "message"
                  ]
                }
              }
            }
          }
        }
      }
    }
